{
  "version": 2,
  "builds": [
    {
      "src": "/apps/backend/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/apps/frontend/.next/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/backend/src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ]
}
